{
    "current_parameters": {
      "label": "",
      "component_source_type": "{{ component.source_type }}",
      "component_source": "{{ component.source }}",
      "component_parameters": {
{% for property in component.properties %}
    {% if property.type|lower == "bool" or property.type|lower == "boolean" %}
      "{{ property.ref }}": {{ property.value|lower }}
    {% elif property.type|lower == "int" or property.type|lower == "integer" or property.type|lower == "number" %}
      "{{ property.ref }}": {{ property.value }}
    {% else %}
      "{{ property.ref }}": "{{ property.value }}"
    {% endif %}
    {% if loop.index != component.properties|length %}
      ,
    {% endif %}
{% endfor %}
      }
    },
    "parameters": [
      {
        "id": "label"
      },
      {
        "id": "component_source_type"
      },
      {
        "id": "component_source"
      },
{% for property in component.properties %}
      {
        "id": "{{ property.ref }}"
      }
    {% if loop.index != component.properties|length %}
      ,
    {% endif %}
{% endfor %}
    ],
    "uihints": {
      "id": "nodeProperties",
      "parameter_info": [
      {
          "parameter_ref": "label",
          "control": "custom",
          "custom_control_id": "StringControl",
          "label": {
            "default": "Label"
          },
          "description": {
            "default": "A custom label for the node.",
            "placement": "on_panel"
          },
          "data": {}
        },
        {
          "parameter_ref": "component_source_type",
          "control": "readonly",
          "label": {
            "default": "Component Source Type"
          },
          "description": {
            "default": "The source of the componenet specification file ('filename' or 'url').",
            "placement": "on_panel"
          },
          "data": {
            "required": true
          }
        },
        {
          "parameter_ref": "component_source",
          "control": "readonly",
          "label": {
            "default": "Path to Component"
          },
          "description": {
            "default": "The path to the component specification file.",
            "placement": "on_panel"
          },
          "data": {
            "required": true
          }
        },
{% for property in component.properties %}
        {
          "parameter_ref": "{{ property.ref }}",
          "control": "{{ property.control }}",
          "custom_control_id": "{{ property.control_id }}",
          "label": {
            "default": "{{ property.name }}"
          },
          "description": {
            "default": "{{ property.description }}",
            "placement": "on_panel"
          },
          "data": {
            "format": "{{ property.type }}",
            "required": {{ property.required|lower }}
            {% if property.control_id == "EnumControl" %}
            ,
            "items": [
              {% for item in property.items %}
              "{{ item }}"
              {% if loop.index != property.items|length %}
              ,
              {% endif %}
              {% endfor %}
              ]
            {% endif %}
          }
        }
      {% if loop.index != component.properties|length %}
        ,
      {% endif %}
{% endfor %}
      ],
      "group_info": [
        {
          "id": "nodeGroupInfo",
          "type": "panels",
          "group_info": [
{% for property in component.properties %}
            {
              "id": "{{ property.ref }}",
              "type": "controls",
              "parameter_refs": ["{{ property.ref }}"]
            }
          {% if loop.index != component.properties|length %}
            ,
          {% endif %}
{% endfor %}
          ]
        }
      ]
    },
    "resources": {}
  }
